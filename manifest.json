{
  "name": "LiveWorksheets",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["scripting", "activeTab", "storage"],
  "action":
  {
    "default_icon": {}
  },
  "background": {
    "service_worker": "dist/background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["https://www.liveworksheets.com/w/en/english-second-language-esl/*"],
      "js": ["dist/controller.js"],
      "world": "MAIN"
    }
  ]
}